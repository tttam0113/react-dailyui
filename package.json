{
    "name": "react-dailyui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "webpack --progress --config webpack.dev.config.js",
        "dev": "webpack-dev-server --config webpack.dev.config.js --progress --colors --content-base dist/",
        "signup-dev": "webpack-dev-server --config webpack.signup.dev.config.js --progress --colors --content-base dist/",
        "voteapp-dev": "webpack-dev-server --config webpack.voteapp.dev.config.js --progress --colors --content-base dist/",
        "timetracking-dev": "webpack-dev-server --config webpack.timetracking.dev.config.js --progress --colors --content-base dist/",
        "checkout-dev": "webpack-dev-server --config webpack.checkout.dev.config.js --progress --colors --content-base dist/",
        "calc-dev": "webpack-dev-server --config webpack.calculator.dev.config.js --progress --colors --content-base dist/",
        "serve": "http-server -p 8000 dist/",
        "lint": "eslint ./src --ext .js,.jsx"
    },
    "dependencies": {
        "react": "^16.4.0",
        "react-dom": "^16.4.0"
    },
    "devDependencies": {
        "autoprefixer": "^8.6.0",
        "babel": "^6.23.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-polyfill": "^6.26.0",
        "babel-preset-react-app": "^3.1.1",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-config-react-app": "^2.1.0",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-flowtype": "^2.49.3",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-react": "^7.9.1",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "mathjs": "^5.0.4",
        "postcss-flexbugs-fixes": "^3.3.1",
        "postcss-loader": "^2.1.5",
        "prop-types": "^15.6.1",
        "react-addons-css-transition-group": "^15.6.2",
        "react-addons-update": "^15.6.2",
        "sass-loader": "^7.0.2",
        "style-loader": "^0.21.0",
        "url-loader": "^1.0.1",
        "uuid": "^3.3.2",
        "webpack": "^4.10.2",
        "webpack-cli": "^3.0.2",
        "webpack-dev-server": "^3.1.4",
        "webpack-merge": "^4.1.2"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
