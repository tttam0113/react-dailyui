{
    "name": "react-dailyui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "webpack --progress --config webpack.dev.config.js",
        "dev": "webpack-dev-server --config webpack.dev.config.js --progress --colors --content-base dist/",
        "signup-dev": "webpack-dev-server --config webpack.signup.dev.config.js --progress --colors --content-base dist/",
        "voteapp-dev": "webpack-dev-server --config webpack.voteapp.dev.config.js --progress --colors --content-base dist/",
        "timetracking-dev": "webpack-dev-server --config webpack.timetracking.dev.config.js --progress --colors --content-base dist/",
        "checkout-dev": "webpack-dev-server --config webpack.checkout.dev.config.js --progress --colors --content-base dist/",
        "calc-dev": "webpack-dev-server --config webpack.calculator.dev.config.js --progress --colors --content-base dist/",
        "serve": "http-server -p 8000 dist/",
        "lint": "eslint ./src --ext .js,.jsx"
    },
    "dependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "autoprefixer": "^9.6.0",
        "babel": "^6.23.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.6",
        "babel-polyfill": "^6.26.0",
        "babel-preset-react-app": "^9.0.0",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-config-react-app": "^4.0.1",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-flowtype": "^3.9.1",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.13.0",
        "file-loader": "^4.0.0",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "immutability-helper": "^3.0.0",
        "mathjs": "^7.5.1",
        "postcss-flexbugs-fixes": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "prop-types": "^15.7.2",
        "react-addons-css-transition-group": "^15.6.2",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "url-loader": "^2.0.0",
        "uuid": "^3.3.2",
        "webpack": "^4.33.0",
        "webpack-cli": "^3.3.3",
        "webpack-dev-server": "^3.7.1",
        "webpack-merge": "^4.2.1"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
